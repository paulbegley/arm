{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
  "parameters": {
    "nsg": {
      "type": "object",
      "defaultValue": { "name": "nsg1" }
    }
  },
    "variables": {
    },
  "resources": [
    {
      "comments": "Generalized from resource: '/subscriptions/50455ae4-c3b0-4fc9-ab2e-425cfec81f46/resourceGroups/rg-backup-poc/providers/Microsoft.Network/networkSecurityGroups/vm-backup-poc-nsg'.",
